@using FileExplorer.DTOs;
@model ProjectDTO
@{
    ViewData["Title"] = "Edit Project";
    Layout = "CustomLayout";
}
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/7.11.0/sweetalert2.css" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/7.11.0/sweetalert2.all.min.js"></script>
<script type="text/javascript" src="~/lib/jquery/dist/jquery.js"></script>
<script type="text/javascript" src="~/lib/jquery/dist/jquery.min.js"></script>



<center style="display:none" id="redirecttoprojectindex">
    <a asp-controller="Project" asp-action="Index" class="btn btn-outline-primary">All Projects</a>

</center>
<br />
<br />

<form method="post"  id="editprojectform" >

    <input type="hidden" asp-for="Id" value="@Model.Id"/>
    <div class="form-group row w-100">
        <label asp-for="ProjectName" for="projecteditformname" >ProjectName</label>
        <input asp-for="ProjectName" id="projecteditformname" type="text" class="form-control form-inline" placeholder="Enter ProjectName">

        <small id="editprojectnc"  >100 /  @Model.ProjectName.Length</small>
         
    </div>
    <br />


    <br />
    <input type="button" id="editprojectsubmit" class="btn btn-success" value="submit" />

</form>



@* <script src="~/js/Project.Edit/ProjectEdit.js"> </script> *@
<script src="~/js/Project.Index/bundle.js"> </script>

